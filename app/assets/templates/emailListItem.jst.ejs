<input type='checkbox' class='email-list-item-check'>
<input type='radio' class='email-list-item-star'>
<p class='email-list-item-sender'>
  <%= email.sender().escape('name') %>
</p>
<p class='email-list-item-subject'>
  <%= email.escape('subject') %>
</p>
<p class='email-list-item-preview'>
  <%= email.preview() %>
</p>
<p class='email-list-item-time-date'>
  <%= email.getTimeOrDate() %>
</p>


































